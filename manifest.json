{
  "name": "Url Appender",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An utility to avoid typing repeated url",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup" : "popup.html"
  },
  "permissions" : [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}